package unipacifico.edu.empresa.logica;

import java.util.ArrayList;
import java.util.List;

public class GestionConvenio {
    private List<PropuestaConvenio> propuestasPendientes;

    public GestionConvenio(){
        this.propuestasPendientes = new ArrayList<>();
    }  
public void agregarPropuesta(PropuestaConvenio propuesta){
    propuestasPendientes.add(propuesta);
}

public void eliminarPropuesta(PropuestaConvenio propuesta){
    propuestasPendientes.remove(propuesta);
}
}
